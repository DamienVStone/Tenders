{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "tenders-sberbank-bidder",
    "namespace": "tenders",
    "labels": {
      "k8s-app": "tenders-sberbank-bidder"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "k8s-app": "tenders-sberbank-bidder"
      }
    },
    "template": {
      "spec": {
        "containers": [
          {
            "name": "tenders-sberbank-bidder",
            "image": "dmitrynikitin/tenders-sberbank-bidder"
          }
        ]
      }
    }
  }
}