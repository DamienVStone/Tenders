{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "bidder",
    "namespace": "tenders",
    "labels": {
      "k8s-app": "bidder"
    },
    "annotations": {
      "deployment.kubernetes.io/revision": "1"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "k8s-app": "bidder"
      }
    },
    "template": {
      "metadata": {
        "name": "bidder",
        "creationTimestamp": null,
        "labels": {
          "k8s-app": "bidder"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "bidder",
            "image": "dmitrynikitin/bidder",
            "command": [
              "mono"
            ],
            "args": [
              "/home/instance/release/Sberbank.exe"
            ],
            "ports": [
              {
                "name": "http",
                "containerPort": 7777,
                "protocol": "TCP"
              }
            ],
            "resources": {},
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "imagePullPolicy": "Always",
            "securityContext": {
              "privileged": false,
              "procMount": "Default"
            }
          }
        ],
        "terminationGracePeriodSeconds": 30,
        "dnsPolicy": "ClusterFirst",
        "securityContext": {},
        "imagePullSecrets": [
          {
            "name": "regcred"
          }
        ],
        "schedulerName": "default-scheduler"
      }
    },
    "strategy": {
      "type": "RollingUpdate",
      "rollingUpdate": {
        "maxUnavailable": "25%",
        "maxSurge": "25%"
      }
    },
    "revisionHistoryLimit": 10,
    "progressDeadlineSeconds": 600
  }
}
{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "bidder",
    "namespace": "tenders",
    "selfLink": "/api/v1/namespaces/tenders/services/bidder",
    "labels": {
      "k8s-app": "bidder"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "tcp-10255-10255-4d5w6",
        "protocol": "TCP",
        "port": 7777,
        "targetPort": "http"
      }
    ],
    "selector": {
      "k8s-app": "bidder"
    },
    "type": "NodePort",
    "sessionAffinity": "None",
    "externalTrafficPolicy": "Cluster"
  },
  "status": {
    "loadBalancer": {}
  }
}